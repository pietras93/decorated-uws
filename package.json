{
  "name": "decorated-uws",
  "version": "0.1.0",
  "description": "Decorator based framework utilizing uws.js",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "NODE_PATH=dist node dist/index.js",
    "dev": "ts-node-dev --clear --respawn -r tsconfig-paths/register --project tsconfig.json src/index.ts",
    "example": "ts-node-dev --clear --respawn -r tsconfig-paths/register example/index.ts"
  },
  "author": "Piotr Frankowski",
  "license": "GNU GPL",
  "dependencies": {
    "@types/node": "^14.11.2",
    "@types/reflect-metadata": "^0.1.0",
    "reflect-metadata": "^0.1.13",
    "ts-node-dev": "^1.0.0-pre.63",
    "tsconfig-paths": "^3.9.0",
    "typedi": "^0.8.0",
    "typescript": "^4.0.3",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v18.5.0"
  }
}
